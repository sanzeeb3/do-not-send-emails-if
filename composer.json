{
  "name": "sanzeeb3/do-not-send-emails-if",
  "description": "Do not send emails if conditions matches. You set the condition.",
  "homepage": "https://github.com/sanzeeb3/do-not-send-emails-if",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=5.6",
    "composer/installers": "1.7.0"
  },
   "autoload": {
    "psr-4": {
        "DoNotSendEmailsIf\\": "src/"
    }
  },
  "scripts": {
    "phpcs": [
      "phpcs -s -p"
    ],
    "phpcs-pre-commit": [
      "phpcs -s -p -n"
    ],
    "phpcbf": [
      "phpcbf -p"
    ]
  }
}